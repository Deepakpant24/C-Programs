#include<stdio.h>
int main()
{
	char _ch;
	printf("enter alp:");
	scanf("%c",&_ch);

	if(_ch=='A'||_ch=='E'||_ch=='I'||_ch=='O'||_ch=='U'||_ch=='a'||_ch=='e'||_ch=='i'||_ch=='o'||_ch=='u')
	{
		printf("%c is a vowel",_ch);
	}
	else if(_ch>='A'&&_ch<='Z'||_ch>='a'&&_ch<='z')
	{
		printf("%c is consonent",_ch);
	}
	else
	{printf("%c is special character",_ch);
	}
	return 0;
}
